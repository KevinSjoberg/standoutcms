<h1>New gallery</h1>

<%= form_for(@gallery) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :website_id %><br />
    <%= f.text_field :website_id %>
  </p>
  <p>
    <%= f.label :content_item_id %><br />
    <%= f.text_field :content_item_id %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', galleries_path %>
