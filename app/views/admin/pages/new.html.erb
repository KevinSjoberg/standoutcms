<h1>New page</h1>

<%= error_messages_for :page %>

<%= form_for(@page) do |f| %>
  <p>
    <b>Website</b><br />
    <%= f.text_field :website_id %>
  </p>

  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Address</b><br />
    <%= f.text_field :address %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pages_path %>
